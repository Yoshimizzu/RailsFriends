<div id="<%= dom_id friend %>">

  <p>
    <strong>Name:</strong>
    <%= link_to friend.first_name + ' ' + friend.last_name , friend %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Instagram:</strong>
    <%= friend.instagram %>
  </p>

   <p>
    <strong>User id:</strong>
    <%= friend.user_id %>
  </p>

</div>
